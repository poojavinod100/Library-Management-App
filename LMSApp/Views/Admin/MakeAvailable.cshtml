
@{
    ViewData["Title"] = "MakeBookAvailable";
}

<h1>Make a Book Available</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form action="PutAvailable">
            <div class="form-group">
                <label>Book Id:</label>
                <input type="text" name="BookId" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Make Available" />
            </div>
        </form>
    </div>
</div>


